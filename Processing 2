//Circles in a Grid

void setup() {
  size(500, 500);
  background(255);
  noStroke();
  smooth();
  colorMode(HSB, 100);

  for (int i = (width/8); i <= (width*1); i = i+30) {
    for (int j = (height/8); j <= (height*1); j = j+30) {
      fill(random(100), 100, 100);
      ellipse(i-22, j-22, 25, 25);
    }
  }
}
